{
  "name": "@ideal-postcodes/postcode-lookup",
  "description": "UK Postcode Lookup plugin from Ideal Postcodes",
  "version": "1.10.2",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "karma start",
    "lint": "eslint lib/**/*.ts",
    "build": "tsc && tsc -p tsconfig.esm.json",
    "watch": "rollup -cw",
    "coverage": "codecov",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run build",
    "docs": "typedoc",
    "test:e2e": "rollup -c && npm run cypress:run",
    "cypress:run": "cypress run",
    "test:browsers:latest": "karma start .config/latest.conf.ts",
    "test:browsers:legacy": "karma start .config/legacy.conf.ts"
  },
  "prettier": {
    "editorconfig": true,
    "trailingComma": "es5"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "browserslist": [
    "ie 11",
    "last 2 versions"
  ],
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=12.0"
  },
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/postcode-lookup/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/postcode-lookup",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/postcode-lookup.git"
  },
  "license": "AGPL-3.0",
  "files": [
    "dist",
    "esm",
    "rollup.config.js"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm",
    "branches": [
      "main",
      "next"
    ]
  },
  "devDependencies": {
    "@babel/cli": "~7.15.4",
    "@babel/core": "~7.16.5",
    "@babel/polyfill": "~7.12.1",
    "@babel/preset-env": "~7.16.5",
    "@cablanchard/eslint-config": "~2.1.0",
    "@cablanchard/semantic-release": "~1.3.4",
    "@cablanchard/tsconfig": "~2.0.0",
    "@ideal-postcodes/api-fixtures": "~1.3.0",
    "@ideal-postcodes/api-typings": "~2.1.0",
    "@ideal-postcodes/doc-assets": "~1.0.6",
    "@ideal-postcodes/supported-browsers": "~2.5.0",
    "@rollup/plugin-commonjs": "~21.0.0",
    "@rollup/plugin-node-resolve": "~13.1.3",
    "@types/chai": "~4.2.14",
    "@types/karma": "~6.3.0",
    "@types/mocha": "~9.0.0",
    "@types/sinon": "~10.0.0",
    "@typescript-eslint/eslint-plugin": "~4.32.0",
    "@typescript-eslint/eslint-plugin-tslint": "~5.8.0",
    "@typescript-eslint/parser": "~4.32.0",
    "@wessberg/rollup-plugin-ts": "~1.3.8",
    "babel-core": "~7.0.0-bridge.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~7.0.0-beta.3",
    "chai": "~4.3.0",
    "chai-dom": "~1.10.0",
    "codecov": "~3.8.0",
    "core-js": "~3.20.1",
    "cypress": "~8.5.0",
    "dotenv": "~8.2.0",
    "eslint": "~7.31.0",
    "eslint-plugin-compat": "~4.0.1",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-mocha": "~2.0.1",
    "karma-sauce-launcher": "~4.3.4",
    "karma-typescript": "~5.5.1",
    "karma-typescript-es6-transform": "~5.3.0",
    "mocha": "~9.1.0",
    "nyc": "~15.1.0",
    "prettier": "~2.5.1",
    "puppeteer": "~10.4.0",
    "rollup": "~2.64.0",
    "semantic-release": "~17.4.0",
    "sinon": "~11.1.1",
    "source-map-support": "~0.5.19",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.3",
    "typedoc": "~0.21.2",
    "typescript": "~4.4.2"
  },
  "dependencies": {
    "@ideal-postcodes/core-axios": "~3.0.5",
    "@ideal-postcodes/jsutil": "~4.6.1"
  }
}
