{
  "name": "@ideal-postcodes/jsutil",
  "version": "4.6.1",
  "description": "Browser Address Autocomplete for api.ideal-postcodes.co.uk",
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/jsutil/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/jsutil",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/jsutil.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "esm/index.js",
  "scripts": {
    "set-env": "node",
    "build": "tsc && tsc -p tsconfig.esm.json",
    "semantic-release": "semantic-release",
    "lint": "eslint lib/**/*.ts",
    "lint-fix": "eslint lib/**/*.ts --fix",
    "coverage": "codecov",
    "docs": "typedoc",
    "prepublishOnly": "npm run build",
    "test": "karma start",
    "test:treeshakeable": "agadoo esm/index.js",
    "test:browsers:latest": "karma start .config/latest.conf.ts",
    "test:browsers:legacy": "karma start .config/legacy.conf.ts"
  },
  "browserslist": [
    "ie 11",
    "last 2 versions",
    "not IE 10"
  ],
  "files": [
    "dist",
    "esm"
  ],
  "prettier": {
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "generate/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts",
      "generate/index.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=10.0"
  },
  "keywords": [
    "API",
    "Interface",
    "UK",
    "Address",
    "Postcode",
    "Search",
    "Ideal Postcodes"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "capitalise-post-town": "~3.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/preset-env": "~7.15.6",
    "@cablanchard/eslint-config": "~2.1.0",
    "@cablanchard/semantic-release": "~1.3.4",
    "@cablanchard/tsconfig": "~2.0.0",
    "@ideal-postcodes/api-fixtures": "~1.3.0",
    "@ideal-postcodes/api-typings": "~2.1.0",
    "@ideal-postcodes/doc-assets": "~1.0.6",
    "@ideal-postcodes/supported-browsers": "~2.5.0",
    "@types/chai": "~4.2.11",
    "@types/karma": "~6.3.0",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.10.2",
    "@types/prettier": "~2.4.1",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "agadoo": "~2.0.0",
    "babel": "~6.23.0",
    "chai": "~4.3.0",
    "codecov": "~3.8.0",
    "core-js": "~3.18.0",
    "dotenv": "~10.0.0",
    "eslint": "~7.31.0",
    "eslint-plugin-compat": "~3.13.0",
    "karma": "~6.3.1",
    "karma-babel-preprocessor": "~8.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-mocha": "~2.0.1",
    "karma-sauce-launcher": "~4.3.3",
    "karma-typescript": "~5.5.1",
    "karma-typescript-es6-transform": "~5.5.2",
    "mocha": "~9.1.1",
    "nyc": "~15.1.0",
    "prettier": "~2.4.1",
    "puppeteer": "~10.4.0",
    "regenerator-runtime": "~0.13.5",
    "semantic-release": "~17.4.0",
    "source-map-support": "~0.5.19",
    "ts-node": "~10.2.0",
    "typedoc": "^0.21.5",
    "typescript": "~4.4.3"
  }
}
